<!-- 日期组件 -->
<template name="Jyo-DateTime">
  <view class="{{_theme}}">
    <view id="{{_id}}" class="Jyo-DateTime" wx:if="{{_isVisible}}" data-id="{{_id}}">
      <view class="Jyo-DateTimeTopBar">
        <view class="Jyo-DateTimeSelectedDateTitle">{{selectedDateTitle}}</view>
        <view class="Jyo-DateTimeLeftArrow" catchtap="{{previousMonthEventId}}"></view>
        <view class="Jyo-DateTimeRightArrow" catchtap="{{nextMonthEventId}}"></view>
        <view class="Jyo-DateTimeWeekFrame">
          <view wx:for="{{weekTitles}}" wx:key="*this">{{item}}</view>
        </view>
      </view>
      <view class="Jyo-DateTimeDateFrame">
        <view wx:for="{{days}}" class="Jyo-DateTimeDay" wx:for-item="day" wx:key="date">
          <view class="{{day.style}}" data-date="{{day.date}}" catchtap="{{selectEventId}}">{{day.day}}</view>
        </view>
      </view>
    </view>
  </view>
</template>

<!-- 网格菜单组件 -->
<template name="Jyo-GridMenuView">
  <view class="{{_theme}}">
    <view id="{{_id}}" class="Jyo-GridMenuView {{_hasBorder?'Jyo-GridMenuViewHasBorder':''}}" wx:if="{{_isVisible}}" data-id="{{_id}}">
      <block wx:for="{{_items}}" wx:key="_id">
        <view class="Jyo-GridMenuViewItem" style="{{item.style}}" data-index="{{item.index}}" data-title="{{item.title}}" data-icon="{{item.icon}}" catchtap="{{itemClickEventId}}">
          <view>
            <image wx:if="{{item.icon}}" src="{{item.icon}}" mode="aspectFit"></image>
            <text>{{item.title}}</text>
          </view>
        </view>
      </block>
    </view>
  </view>
</template>

<!-- HTML视图组件 -->
<template name="Jyo-HTMLView">
  <view class="{{_theme}}">
    <view id="{{_id}}" class="Jyo-HTMLView" wx:if="{{_isVisible}}" data-id="{{_id}}" style="width:{{_width}};padding:0 {{_padding}};">
      <block wx:for="{{nodes}}" wx:key="">
        <template is="Jyo-HTMLView0" data="{{item}}" />
      </block>
    </view>
  </view>
</template>



<!--基础元素-->
<template name="Jyo-HTMLView-Video">
  <view bindtap="{{item.tapEvent}}" wx:if="{{item.hidden!=true}}" class="{{item.cls}} Jyo-HTMLView-{{item.tag}}" style="{{item.styleStr}}">
    <video class="{{item.cls}} Jyo-HTMLView-{{item.tag}}-video" src="{{item.attr.src}}"></video>
  </view>
</template>

<template name="Jyo-HTMLView-Img">
  <image wx:if="{{item.hidden!=true}}" class="{{item.cls}} Jyo-HTMLView-{{item.tag}}" data-src="{{item.attr.src}}" data-idx="{{item.ii}}" src="{{item.attr.src}}" mode="aspectFit" bindload="{{item.imageLoadEventId}}" bindtap="{{item.imageClickEventId}}"
    mode="{{item.processImg?'widthFix':'scaleToFill'}}" style="width:{{item.width}}px;{{item.styleStr}}" />
</template>

<template name="Jyo-HTMLView-Text">
  <view bindtap="{{item.tapEvent}}" wx:if="{{item.hidden!=true}}" class="Jyo-HTMLView-inline" style="{{item.styleStr}}">
    <block class="{{item.text == '\\n' ? 'Jyo-HTMLView-hide':''}}" wx:if="{{item.n== 'txt'}}">{{item.txt}}</block>
  </view>
</template>




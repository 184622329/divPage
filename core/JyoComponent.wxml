          </view>
          <view wx:if="{{item.hidden!=true}}" class="{{item.cls}} Jyo-HTMLView-li-text">
            <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
              <template is="Jyo-HTMLView1" data="{{item}}" />
            </block>
          </view>
        </view>
      </view>
    </block>

    <!--video类型-->
    <block wx:elif="{{item.tag == 'video'}}">
      <template is="Jyo-HTMLView-Video" data="{{item}}" />
    </block>

    <!--img类型-->
    <block wx:elif="{{item.tag == 'img'}}">
      <template is="Jyo-HTMLView-Img" data="{{item}}" />
    </block>

    <!--a类型-->
    <block wx:elif="{{item.tag == 'a'}}">
      <view wx:if="{{item.hidden!=true}}" bindtap="{{item.linkClickEventId}}" class="Jyo-HTMLView-inline {{item.cls}} Jyo-HTMLView-{{item.tag}}" data-src="{{item.attr.href}}" data-target="{{item.attr.target}}" style="{{item.styleStr}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="Jyo-HTMLView1" data="{{item}}" />
        </block>
      </view>
    </block>
    <block wx:elif="{{item.tag == 'table'}}">
      <view wx:if="{{item.hidden!=true}}" class="{{item.cls}} Jyo-HTMLView-{{item.tag}}" style="{{item.styleStr}}" bindtap="{{item.tapEvent}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="Jyo-HTMLView1" data="{{item}}" />
        </block>
      </view>
    </block>

    <!--其他块级标签-->
    <block wx:elif="{{item.tt == 'block'}}">
      <view wx:if="{{item.hidden!=true}}" class="{{item.cls}} Jyo-HTMLView-{{item.tag}}" style="{{item.styleStr}}" bindtap="{{item.tapEvent}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="Jyo-HTMLView1" data="{{item}}" />
        </block>
      </view>
    </block>

    <!--内联标签-->
    <view wx:else class="{{item.cls}} Jyo-HTMLView-{{item.tag}} Jyo-HTMLView-{{item.tt}}" style="{{item.styleStr}}">
      <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
        <template is="Jyo-HTMLView1" data="{{item}}" />
      </block>
    </view>

  </block>

  <!--判断是否是文本节点-->
  <block wx:elif="{{item.n== 'txt'}}">
    <!--如果是，直接进行-->
    <template is="Jyo-HTMLView-Text" data="{{item}}" />
  </block>

</template>


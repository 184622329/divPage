      <view wx:if="{{item.hidden!=true}}" class="{{item.cls}} Jyo-HTMLView-{{item.tag}}" style="{{item.styleStr}}" bindtap="{{item.tapEvent}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="Jyo-HTMLView20" data="{{item}}" />
        </block>
      </view>
    </block>

    <!--其他块级标签-->
    <block wx:elif="{{item.tt == 'block'}}">
      <view wx:if="{{item.hidden!=true}}" class="{{item.cls}} Jyo-HTMLView-{{item.tag}}" style="{{item.styleStr}}" bindtap="{{item.tapEvent}}">
        <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
          <template is="Jyo-HTMLView20" data="{{item}}" />
        </block>
      </view>
    </block>

    <!--内联标签-->
    <view wx:else class="{{item.cls}} Jyo-HTMLView-{{item.tag}} Jyo-HTMLView-{{item.tt}}" style="{{item.styleStr}}">
      <block wx:for="{{item.nodes}}" wx:for-item="item" wx:key="">
        <template is="Jyo-HTMLView20" data="{{item}}" />
      </block>
    </view>

  </block>

  <!--判断是否是文本节点-->
  <block wx:elif="{{item.n== 'txt'}}">
    <!--如果是，直接进行-->
    <template is="Jyo-HTMLView-Text" data="{{item}}" />
  </block>

</template>

<!-- 搜索栏组件 -->
<template name="Jyo-SearchBar">
  <view class="{{_theme}}">
    <view id="{{_id}}" class="Jyo-SearchBar {{_isEditorStyle}}" wx:if="{{_isVisible}}" data-id="{{_id}}">
      <scroll-view wx:if="{{_isEditorStyle}}" class="Jyo-SearchBarFrameMask" catchtap="{{clickFrameMaskEventId}}" scroll-y="true" scroll-with-animation="true" enable-back-to-top="true">
        <view wx:if="{{!_list.length&&!!_history.length&&!_currentValue}}" class="Jyo-SearchBarHistory">
          <view class="Jyo-SearchBarTitle">{{_historyTitle}}
            <view class="Jyo-SearchBarBtnClear" catchtap="{{clearHistoryEventId}}">{{_clearText}}</view>
          </view>
          <block wx:for="{{_history}}" wx:key="title">
            <view class="Jyo-SearchBarItem {{item.hasBorder?'Jyo-SearchBarHasBorder':''}}" data-title="{{item.title}}" data-desc="{{item.desc}}" data-other="{{item.other}}" catchtap="{{itemSelectEventId}}">{{item.title}}
              <view wx:if="{{item.desc}}" class="Jyo-SearchBarDesc">{{item.desc}}</view>
            </view>
          </block>
        </view>
        <block wx:for="{{_list}}" wx:key="title">
          <view class="Jyo-SearchBarItem {{item.hasBorder?'Jyo-SearchBarHasBorder':''}}" data-title="{{item.title}}" data-desc="{{item.desc}}" data-other="{{item.other}}" catchtap="{{itemSelectEventId}}">{{item.title}}
            <view wx:if="{{item.desc}}" class="Jyo-SearchBarDesc">{{item.desc}}</view>
          </view>
        </block>
      </scroll-view>
      <view class="Jyo-SearchBarInputFrame" style="width:{{_inputWidth}}rpx;" catchtap="{{foucsEventId}}">
        <view class="Jyo-SearchBarSearchIcon"></view>
        <view class="Jyo-SearchBarDefText">{{defText}}</view>
        <input wx:if="{{_isEditorStyle}}" focus="true" confirm-type="search" placeholder="{{defText}}" bindinput="{{inputEventId}}" />
      </view>
      <view wx:if="{{_isEditorStyle}}" class="Jyo-SearchBarBtnCancel" catchtap="{{cancelEventId}}">{{_cancelText}}</view>
    </view>
  </view>
</template>

<!-- 排列滚动视图组件 -->
<template name="Jyo-SortListView">
  <view class="{{_theme}}">
    <scroll-view id="{{_id}}" class="Jyo-SortListView" wx:if="{{_isVisible}}" data-id="{{_id}}" scroll-y="true" scroll-with-animation="true" enable-back-to-top="true" scroll-into-view="{{toFast}}">
      <block wx:for="{{list}}" wx:key="title">
        <view class="{{item.ict===1?'Jyo-SortListViewClassificationTitle':'Jyo-SortListViewItem'}} {{item.hb===1?'Jyo-SortListViewHasBorder':''}}" id="{{item.id}}" data-title="{{item.title}}" data-desc="{{item.desc}}" data-other="{{item.other}}" catchtap="{{selectEventId}}"
          data-canClick="{{item.cc}}">{{item.title}}
          <view wx:if="{{item.desc}}" class="Jyo-SortListViewDesc">{{item.desc}}</view>
        </view>
      </block>
      <view class="Jyo-SortListViewFooterText" wx:if="{{_footerText}}">{{_footerText}}</view>
    </scroll-view>
    <view class="Jyo-SortListView-FastBar" wx:if="{{_isVisible}}" catchtouchstart="{{fastEventId}}" catchtap="{{fastEventId}}">
      <view class="Jyo-SortListView-FastBarFrame">
        <view wx:for="{{fastList}}" wx:key="*this" data-value="{{item}}">{{item}}</view>
      </view>
    </view>
  </view>
</template>

